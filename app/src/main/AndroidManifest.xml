<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lcp.arecyclerview">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.ListMoreActivity" />
        <activity android:name=".activity.ListMoreActivity2" />
        <activity android:name=".activity.GridMoreActivity" />
        <activity android:name=".activity.GridMoreActivity2" />
        <activity android:name=".activity.HeaderActivity" />
        <activity android:name=".activity.ScrollerActivity" />
        <activity android:name=".activity.ItemTouchHelperActivity"/>
    </application>

</manifest>